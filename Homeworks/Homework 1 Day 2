import random 
# asal sayıları çıkardık
mylist= []
for x in range(1,100):
  for i in range(2,x):
    if (x%i)== 0:
      break
  else:
    mylist.append(x) 
# matris üretme
for x in range (1,4):
  print(mylist[random.randint(0,len(mylist)-1)], mylist[random.randint(0,len(mylist)-1)],mylist[random.randint(0,len(mylist)-1)])
